<div class="form-group">
    <label>{{localize 'ckl-roll-bonuses.spells.elementalFocusEnergy'}}</label>
    <div class="form-fields">
        <select damageType="school" id="elemental-focus-selector">
            {{#select element}}
            {{#each elements as |damageType elem|}}
            {{log 'damageType' damageType}}
            <option value="{{elem}}">{{damageType.name}}</option>
            {{/each}}
            {{/select}}
        </select>
    </div>
</div>